{
  "name": "code-equality-assertions",
  "version": "0.9.0",
  "description": "",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./qunit": {
      "import": {
        "types": "./dist/qunit.d.ts",
        "default": "./dist/qunit.js"
      },
      "require": {
        "types": "./dist/qunit.d.ts",
        "default": "./dist/qunit.cjs"
      }
    },
    "./jest": {
      "import": {
        "types": "./dist/jest.d.ts",
        "default": "./dist/jest.js"
      },
      "require": {
        "types": "./dist/jest.d.ts",
        "default": "./dist/jest.cjs"
      }
    }
  },
  "repository": "https://github.com/ef4/code-equality-assertions",
  "author": "Edward Faulkner <edward@eaf4.com>",
  "scripts": {
    "test": "NODE_OPTIONS='--loader ts-node/esm' qunit tests/*-test.ts",
    "build": "tsup",
    "example:jest-node": "NODE_OPTIONS='--experimental-vm-modules' ./node_modules/.bin/jest --config examples/jest/jest.config.cjs",
    "example:qunit-node": "NODE_OPTIONS='--loader ts-node/esm' qunit examples/qunit/example-test.ts",
    "example:qunit-browser": "cd examples/qunit && webpack serve"
  },
  "files": [
    "dist",
    "index.d.ts",
    "jest.d.ts",
    "qunit.d.ts"
  ],
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@types/babel__core": "^7.1.19",
    "@types/diff": "^5.0.2",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.7.18",
    "@types/prettier": "^2.7.0",
    "@types/qunit": "^2.19.2",
    "css-loader": "^6.7.1",
    "jest": "^29.2.1",
    "qunit": "2.19.1",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "dependencies": {
    "@babel/core": "^7.19.6",
    "diff": "^5.1.0",
    "prettier": "^2.7.1"
  },
  "peerDependencies": {
    "@types/jest": "2",
    "@types/qunit": "2",
    "qunit": "2"
  },
  "peerDependenciesMeta": {
    "@types/qunit": {
      "optional": true
    },
    "@types/jest": {
      "optional": true
    },
    "qunit": {
      "optional": true
    }
  },
  "tsup": {
    "dts": true,
    "format": [
      "esm",
      "cjs"
    ],
    "entry": [
      "src/index.ts",
      "src/qunit.ts",
      "src/jest.ts"
    ]
  }
}
